{
    "name": "rmezhuev/data-transfer-object",
    "description": "Plain and simple immutable DTO pattern implementation based on php annotations",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruslan Mezhuev",
            "email": "r.mezhuev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "symfony/property-info": "^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/var-dumper": "^5.0",
        "fakerphp/faker": "^1.9.1"
    },
    "autoload": {
        "psr-4": {
            "RMezhuev\\DTO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RMezhuev\\DTO\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
